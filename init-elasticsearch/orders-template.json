{
  "index_patterns": ["ecommerce-orders*"],
  "template": {
    "mappings": {
      "properties": {
        "id": { "type": "integer" },
        "userId": { "type": "integer" },
        "date": { "type": "date" },
        "products": {
          "type": "nested",
          "properties": {
            "productId": { "type": "integer" },
            "quantity": { "type": "integer" }
          }
        },
        "total_items": { "type": "integer" },
        "ingest_ts": { "type": "date", "format": "epoch_second" }
      }
    }
  }
}
